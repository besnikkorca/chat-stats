{
  "private": "true",
  "name": "server",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "seed": "ts-node src/db/seeders/DatabaseSeeder.ts",
    "start": "ts-node src/index.ts",
    "start:dev": "nodemon src/index.ts",
    "start:build": "node build/index.js",
    "build": "run-s clean compile copy",
    "clean": "rm -rf build",
    "compile": "tsc --outDir build",
    "copy": "cp src/schema.graphql build/schema.graphql",
    "test": "run-s eslint:check prettier:check  jest",
    "jest": "jest",
    "eslint:check": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "prettier:write": "prettier --ignore-path ../.prettierignore --write \"**/*.{js,jsx,ts,tsx,json,md,css,scss}\"",
    "prettier:check": "prettier --ignore-path ../.prettierignore --list-different \"**/*.{js,jsx,ts,tsx,json,md,css,scss}\""
  },
  "dependencies": {
    "dotenv": "^16.0.2",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "jest": "^29.1.1"
  }
}
